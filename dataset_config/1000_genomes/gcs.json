// Fields for GCS indexer
{
  "gcs_patterns": [
    // GCS path with the string "PRIMARY_KEY" indicating where primary key
    // exists in the path. For example, if your GCS file path looks like
    // gs://genomics-public-data/platinum-genomes/bam/NA12877_S1.bam, then
    // you would put
    // gs://genomics-public-data/platinum-genomes/bam/PRIMARY_KEY_
    // For more information about primary key, see
    // https://github.com/DataBiosphere/data-explorer-indexers#overview
    // This may include regex syntax such as *
    "gs://genomics-public-data/1000-genomes/bam/PRIMARY_KEY.",
    "gs://genomics-public-data/ftp-trace.ncbi.nih.gov/1000genomes/ftp/data/PRIMARY_KEY"
  ]
}
