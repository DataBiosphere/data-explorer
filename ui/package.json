{
  "name": "data-explorer",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@material-ui/core": "^1.5.1",
    "@material-ui/icons": "^2.0.3",
    "ajv": "^6.5.2",
    "data_explorer_service": "file:src/api",
    "husky": "^0.14.3",
    "material-ui": "^0.20.1",
    "prettier": "^1.13.7",
    "pretty-quick": "^1.6.0",
    "react": "^16.3.0",
    "react-dom": "^16.3.0",
    "react-scripts": "^1.1.4"
  },
  "scripts": {
    "lint": "prettier **/*.js **/*.css",
    "fix": "prettier --write **/*.js **/*.css",
    "precommit": "pretty-quick --staged",
    "start": "PORT=4400 react-scripts start",
    "build": "react-scripts build",
    "test": "jest tests/unit",
    "update": "jest tests/unit --updateSnapshot",
    "test:e2e": "jest --config '{\"preset\":\"jest-puppeteer\"}' tests/e2e",
    "eject": "react-scripts eject"
  },
  "devDependencies": {
    "babel-plugin-transform-export-extensions": "^6.22.0",
    "babel-preset-env": "^1.7.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "enzyme-to-json": "^3.3.4",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^22.4.4",
    "jest-cli": "^22.4.4",
    "jest-environment-node": "^22.0.0",
    "jest-puppeteer": "^2.4.0",
    "puppeteer": "^1.6.0",
    "react-test-renderer": "^16.4.1"
  },
  "optionalDependencies": {
    "fsevents": "*"
  },
  "//": [
    "After https://github.com/facebook/jest/issues/6026 is fixed",
    "create jest unit and e2e projects and move preset=jest-puppeteer",
    "setting to e2e project."
  ],
  "jest": {
    "coveragePathIgnorePatterns": [
      "<rootDir>/src/api/"
    ],
    "modulePathIgnorePatterns": [
      "<rootDir>/src/api/"
    ],
    "moduleNameMapper": {
      "^.+\\.(css|scss)$": "identity-obj-proxy"
    },
    "setupFiles": [
      "./tests/unit/jestsetup.js"
    ],
    "snapshotSerializers": [
      "enzyme-to-json/serializer"
    ]
  }
}
